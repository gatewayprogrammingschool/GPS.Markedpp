{
    "name": "gps-markedpp",
    "displayName": "GPS.Markedpp",
    "description": "VSCode Addin for applying preprocessor transforms for Markdown via markedpp.",
    "version": "1.0.0",
    "publisher": "GatewayProgrammingSchoolInc",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:gps-markdown.transformMarkdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "gps-markdown.transformMarkdown",
                "title": "GPS: Transform Markdown"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "markedpp": "^0.3.10",
        "typescript-string-operations": "^1.3.1",
        "vsce": "^1.48.0"
    }
}
